msgid "Add Directory"
msgstr "Přidat adresář"

#: src/panucci/panucci.py:1121
#, python-format
msgid "Bookmark added: %s"
msgstr "Záložka přidána: %s"

#: src/panucci/panucci.py:1203
msgid "Playlist item details"
msgstr "Podrobnosti k položce seznamu skladeb"

#: src/panucci/panucci.py:1221
msgid "Custom title:"
msgstr "Uživatelsky stanovený název:"

#: src/panucci/panucci.py:1222
msgid "ID:"
msgstr "ID:"

